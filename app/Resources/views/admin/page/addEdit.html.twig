{% form_theme form '::twig/formRow.html.twig' %}
{% extends "::base.html.twig" %}

{% block body %}
    {{ form_start(form) }}
        {{ form_errors(form) }}
        {{ form_row(form.title) }}
        {{ form_row(form.url) }}
        {{ form_row(form.content) }}

        <button class="btn btn-primary ladda-button" type="submit">
            {{ 'action.save'|trans }}
        </button>
    {{ form_end(form) }}
{% endblock %}