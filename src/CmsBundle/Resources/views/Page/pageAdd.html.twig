{% form_theme form '::twig/formRow.html.twig' %}
{% extends "::base.html.twig" %}

{% block body %}
    
    {{ form_start(form) }}
        {{ form_errors(form) }}
        {{ form_row(form.title) }}
        {{ form_row(form.url) }}
        {{ form_row(form.content) }}
        {{ form_row(form.save) }}
        {{ form_row(form.saveAndAdd) }}
    {{ form_end(form) }}
    
{% endblock %}